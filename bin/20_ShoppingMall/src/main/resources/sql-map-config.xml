<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.green.biz.dto.ProductVO" alias="product"></typeAlias>
		<typeAlias type="com.green.biz.dto.MemberVO" alias="member"></typeAlias>
		<typeAlias type="com.green.biz.dto.AddressVO" alias="address"></typeAlias>
		<typeAlias type="com.green.biz.dto.CartVO" alias="cart"></typeAlias>
		<typeAlias type="com.green.biz.dto.OrderVO" alias="order"></typeAlias>
		<typeAlias type="com.green.biz.dto.QnaVO" alias="qna"></typeAlias>
		<typeAlias type="com.green.biz.dto.WorkerVO" alias="worker"></typeAlias>
	</typeAliases>
	
	<mappers>
		<mapper resource="mappings/product-mapping.xml" />
		<mapper resource="mappings/member-mapping.xml" />
		<mapper resource="mappings/cart-mapping.xml" />
		<mapper resource="mappings/order-mapping.xml" />
		<mapper resource="mappings/qna-mapping.xml" />
		<mapper resource="mappings/admin-mapping.xml" />
	</mappers>
</configuration>
